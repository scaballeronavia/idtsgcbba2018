{#
/**
 * @file mueble.html.twig
 * Default theme implementation to present Mueble data.
 *
 * This template is used when viewing Mueble pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_mueble()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('mueble') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
